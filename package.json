{
  "name": "tutorial",
  "version": "1.0.0",
  "description": "tutorial of a express server",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "generate": "npx prisma generate --watch",
    "migrate:dev": "npx prisma migrate dev --skip-seed",
    "db:push": "npx prisma db push",
    "dev": "nodemon server.js",
    "dev-docker": "npm run generate && nodemon --legacy-watch server.js",
    "docker:dev": "docker compose up -d && docker logs -f node_container",
    "docker:build": "docker compose up -d --build && docker logs -f node_container"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.js",
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "dbdocs": "^0.8.4",
    "generate-changelog": "^1.8.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.0"
  }
}
